<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<!--

    Configuration file for UrlRewriteFilter
    http://www.tuckey.org/urlrewrite/

-->
<urlrewrite>
    <rule>
        <from>^/login$</from>
        <to>/login.jsp</to>
    </rule>
    <rule>
        <from>^/qualification$</from>
        <to>/qualification.jsp</to>
    </rule>
    <rule>
        <from>^/404$</from>
        <to>/404.jsp</to>
    </rule>
    <rule>
        <from>^/job$</from>
        <to>/job.jsp</to>
    </rule>
    <rule>
        <from>^/user-profile$</from>
        <to>/user-profile.jsp</to>
    </rule>
    <rule>
        <from>^/user$</from>
        <to>/user.jsp</to>
    </rule>
    <rule>
        <from>^/engineer-job-mapping$</from>
        <to>/engineer-job-mapping.jsp</to>
    </rule>
    <rule>
        <from>^/engineer-qualification-mapping$</from>
        <to>/engineer-qualification-mapping.jsp</to>
    </rule>
    <rule>
        <from>^/handler/data$</from>
        <to>/handler/data.jsp</to>
    </rule>
    <rule>
        <from>^/handler/login$</from>
        <to>/handler/login.jsp</to>
    </rule>
    <rule>
        <from>^/handler/logout$</from>
        <to>/handler/logout.jsp</to>
    </rule>
    <rule>
        <from>^/handler/mapping$</from>
        <to>/handler/mapping.jsp</to>
    </rule>
</urlrewrite>

